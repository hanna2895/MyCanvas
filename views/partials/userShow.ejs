<div class="content-header"><h3><%=user.username%>'s Canvas</h3></div>

<div class="photo-holder">


  <% if(photos) {%>

    <% for (let i = photos.length-1; i >= 0; i--) { -%>
      <% if (photos[i].url) {%>

        <a href="/photos/<%=photos[i]._id%>" class="img-link"><img src="<%= user.photos[i].url%>"></a>

      <%} %>
    <% } -%>

  <% } -%>
</div>
